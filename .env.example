# GoHighLevel API Configuration
# Get your API key from: https://app.gohighlevel.com/settings/integrations
GHL_API_KEY=your_gohighlevel_api_key_here

# GoHighLevel Webhook Secret (for webhook signature verification)
# Set this to a secure random string and use the same value in GHL webhook config
GHL_WEBHOOK_SECRET=your_webhook_secret_here

# CallTools API Configuration
# Get your API key from your CallTools account settings
CALLTOOLS_API_KEY=your_calltools_api_key_here

# CallTools Base URL (optional, defaults to https://api.calltools.com/v1)
CALLTOOLS_BASE_URL=https://api.calltools.com/v1
